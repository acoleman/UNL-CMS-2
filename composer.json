{
    "name": "drupal/drupal",
    "description": "Drupal is an open source content management platform powering millions of websites and applications.",
    "type": "project",
    "license": "GPL-2.0+",
    "require": {
        "composer/installers": "^1.0.21",
        "wikimedia/composer-merge-plugin": "~1.3",
        "cweagans/composer-patches": "~1.0",
        "drupal/token": "^1.0",
        "drupal/ctools": "3.0-alpha27",
        "drupal/pathauto": "^1.0",
        "drupal/diff": "^1.0",
        "drupal/memcache": "2.0-alpha2",
        "drupal/menu_block": "1.4",
        "drupal/metatag": "1.0-beta11",
        "drupal/file_entity": "2.0-beta3",
        "drupal/scheduler": "1.0-alpha2",
        "unlcms/unl_fourone": "*",
        "unlcms/unl_cas": "*",
        "unlcms/unl_filter": "*",
        "unlcms/unl_purge_processor": "*",
        "unlcms/unl_user": "*",
        "unlcms/unl_breadcrumbs": "dev-master",
        "drupal/config_installer": "1.3",
        "drupal/drupal_reset": "1.x-dev",
        "drupal/eck": "1.0-alpha2",
        "drupal/inline_entity_form": "1.0-beta1",
        "drupal/easy_breadcrumb": "1.5",
        "drupal/devel": "1.0-beta1",
        "drupal/imce": "1.4",
        "drupal/admin_toolbar": "1.18",
        "drupal/honeypot": "^1.23",
        "drupal/webform": "^5.0.0",
        "drupal/paragraphs": "1.0",
        "drupal/entity_reference_revisions": "^1.0",
        "drupal/exclude_node_title": "1.0-beta1",
        "drupal/crop": "1.0",
        "drupal/focal_point": "1.0-beta4",
        "drupal/purge": "^3.0@beta",
        "drupal/varnish_purge": "^1.4",
        "symfony/ldap": "~3.2.1",
        "drupal/group": "1.x-dev"
    },
    "replace": {
        "drupal/core": "~8.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "autoloader-suffix": "Drupal8"
    },
    "extra": {
        "_readme": [
            "By default Drupal loads the autoloader from ./vendor/autoload.php.",
            "To change the autoloader you can edit ./autoload.php."
        ],
        "merge-plugin": {
            "include": [
                "core/composer.json"
            ],
            "recurse": false,
            "replace": false,
            "merge-extra": false
        },
        "installer-paths": {
            "core": ["type:drupal-core"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/group": {
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
        }
    },
    "scripts": {
        "pre-autoload-dump": "Drupal\\Core\\Composer\\Composer::preAutoloadDump",
        "post-autoload-dump": [
          "Drupal\\Core\\Composer\\Composer::ensureHtaccess",
          "Drupal\\Core\\Composer\\Composer::configurePhpcs"
        ],
        "post-package-install": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup",
        "post-package-update": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "unl_fourone": {
            "url": "https://github.com/unlcms/unl_fourone.git",
            "type": "git"
        },
        "unl_cas": {
            "url": "https://github.com/unlcms/unl_cas.git",
            "type": "git"
        },
        "unl_filter": {
            "url": "https://github.com/unlcms/unl_filter.git",
            "type": "git"
        },
        "unl_purge_processor": {
            "url": "https://github.com/unlcms/unl_purge_processor.git",
            "type": "git"
        },
        "unl_user": {
          "url": "https://github.com/unlcms/unl_user.git",
          "type": "git"
        },
        "unl_breadcrumbs": {
          "url": "https://github.com/unlcms/unl_breadcrumbs.git",
          "type": "git"
        }
    }
}
